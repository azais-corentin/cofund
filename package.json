{
  "name": "cofund",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "bun --bun vite dev",
    "build": "bun --bun vite build",
    "preview": "bun run build && wrangler dev",
    "format": "dprint fmt",
    "format:check": "dprint check",
    "lint": "bun --bun oxlint --type-aware",
    "prepare": "bun --bun svelte-kit sync",
    "check": "bun run prepare && bun --bun svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "bun run check --watch",
    "deploy": "npm run build && bun --bun wrangler deploy",
    "cf-typegen": "bun --bun wrangler types ./src/worker-configuration.d.ts"
  },
  "devDependencies": {
    "@fontsource-variable/roboto": "^5.2.8",
    "@internationalized/date": "^3.10.0",
    "@lucide/svelte": "^0.552.0",
    "@sveltejs/adapter-cloudflare": "^7.2.4",
    "@sveltejs/kit": "^2.48.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/vite": "^4.1.16",
    "@types/bun": "^1.3.1",
    "@vite-pwa/assets-generator": "^1.0.2",
    "@vite-pwa/sveltekit": "^1.0.1",
    "bits-ui": "^2.14.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.19",
    "formsnap": "^2.0.1",
    "jazz-tools": "^0.18.33",
    "mode-watcher": "^1.1.0",
    "oxlint": "^1.25.0",
    "oxlint-tsgolint": "^0.4.0",
    "sharp": "^0.34.4",
    "sharp-ico": "^0.1.5",
    "svelte": "^5.43.2",
    "svelte-check": "^4.3.3",
    "svelte-sonner": "^1.0.5",
    "sveltekit-superforms": "^2.28.0",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.1.1",
    "tailwindcss": "^4.1.16",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vite-plugin-mkcert": "^1.17.9",
    "wrangler": "^4.45.3",
    "zod": "^4.1.12"
  }
}
