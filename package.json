{
  "name": "cofund",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "bun vite dev",
    "dev:ws": "bun run ws-server.ts",
    "dev:all": "bun run dev:ws & bun run dev",
    "build": "bun vite build",
    "preview": "bun vite preview",
    "prepare": "bun svelte-kit sync || echo ''",
    "format": "dprint fmt",
    "lint": "oxlint --type-aware",
    "check": "bun run prepare && bun svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "bun run check --watch"
  },
  "devDependencies": {
    "@fontsource-variable/roboto": "^5.2.8",
    "@internationalized/date": "^3.10.0",
    "@jesterkit/exe-sveltekit": "^0.1.5",
