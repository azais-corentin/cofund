    "oxlint-tsgolint": "^0.2.0",
    "sharp": "^0.34.4",
    "sharp-ico": "^0.1.5",
    "svelte": "^5.40.0",
    "svelte-check": "^4.3.3",
    "sveltekit-superforms": "^2.27.3",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.1.1",
    "tailwindcss": "^4.1.14",
    "tinybase": "^6.7.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "valibot": "^1.1.0",
    "vite": "^7.1.10",
    "ws": "^8.18.3"
  }
