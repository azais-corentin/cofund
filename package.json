    "@logtape/pretty": "^1.1.1",
    "@lucide/svelte": "^0.545.0",
    "@sveltejs/kit": "^2.46.5",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/vite": "^4.1.14",
    "@types/bun": "^1.3.0",
    "@types/ws": "^8.18.1",
    "@vite-pwa/assets-generator": "^1.0.2",
    "@vite-pwa/sveltekit": "^1.0.1",
    "bits-ui": "^2.11.5",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.18",
    "formsnap": "^2.0.1",
    "mode-watcher": "^1.1.0",
