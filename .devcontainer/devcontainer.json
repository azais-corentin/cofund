{
  "name": "cofund devcontainer",
  "dockerComposeFile": "docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
  "customizations": {
    "vscode": {
      "extensions": [
        "Gxl.git-graph-3",
        "svelte.svelte-vscode",
        "bradlc.vscode-tailwindcss",
        "fill-labs.dependi",
        "oxc.oxc-vscode",
        "dprint.dprint"
      ]
    }
  },
  "forwardPorts": ["db:5432"]
}
