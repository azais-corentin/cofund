{
  "dockerfile": {},
  "indentWidth": 2,
  "json": {},
  "lineWidth": 100,
  "malva": {
    "quotes": "preferSingle"
  },
  "markdown": {},
  "markup": {
    "component.selfClosing": true,
    "doctypeKeywordCase": "lower",
    "html.normal.selfClosing": false,
    "preferAttrsSingleLine": true,
    "scriptIndent": true,
    "styleIndent": true
  },
  "toml": {},
  "typescript": {
    "arrayExpression.preferHanging": "always",
    "memberExpression.linePerExpression": true,
    "quoteStyle": "preferSingle"
  },
  "useTabs": false,
  "yaml": {
    "quotes": "preferSingle"
  },
  "excludes": ["**/*.lock", "**/node_modules", "**/dist"],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.95.12.wasm",
    "https://plugins.dprint.dev/json-0.21.0.wasm",
    "https://plugins.dprint.dev/markdown-0.20.0.wasm",
    "https://plugins.dprint.dev/toml-0.7.0.wasm",
    "https://plugins.dprint.dev/dockerfile-0.3.3.wasm",
    "https://plugins.dprint.dev/g-plane/malva-v0.14.3.wasm",
    "https://plugins.dprint.dev/g-plane/markup_fmt-v0.24.0.wasm",
    "https://plugins.dprint.dev/g-plane/pretty_yaml-v0.5.1.wasm"
  ]
}
